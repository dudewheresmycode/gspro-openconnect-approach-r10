{
  "name": "gspro-openconnect-approach-r10",
  "version": "1.1.7",
  "description": "GSPro OpenConnect for Garmin Approach R10",
  "main": "electron/main.js",
  "scripts": {
    "react": "react-scripts start",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "watch": "webpack watch --env mode=development",
    "debug": "npm run webpack-dev-server -- --env mode=development",
    "build": "webpack --env mode=production",
    "build-win": "electron-builder --windows",
    "build-mac": "electron-builder --mac",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder --windows --publish always"
  },
  "build": {
    "appId": "com.dudewheresmycode.gspro-openconnect-approach-r10",
    "productName": "GSPro Connect R10",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "./assets/icon/icon.icns"
    },
    "extends": null,
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.map",
      "!package.json",
      "!package-lock.json",
      {
        "from": "../webpack",
        "filter": [
          "**/*"
        ]
      }
    ],
    "dmg": {
      "icon": false
    },
    "win": {
      "icon": "./assets/icon/icon.png"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^6.8.1",
    "electron": "^23.0.0",
    "electron-builder": "^23.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "^2.8.4",
    "react-scripts": "^5.0.1",
    "svg-url-loader": "^8.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "address": "^1.2.2",
    "lowdb": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sound-play": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 safari version"
    ]
  }
}
